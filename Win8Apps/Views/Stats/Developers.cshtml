@{
    ViewBag.Title = "WinApps - Developers statistics";
}

<div class="page-region">
    <div class="page-region-content">
        <div class="grid">
            <div class="row">
                <div class="span12 bg-color-green">
                    <i class="fg-color-white icon-user-3 place-right" style="margin: 10px; margin-top: 18px;  font-size: 30px"></i>
                    <h2 class="fg-color-white">&nbsp;Top Developers</h2>
                </div>
            </div>
            <div class="row">
                <div class="span12">
                    @{
                        Html.RenderAction("TopDevelopers", "Stats");
                    }
                </div>
            </div>
        </div>
    </div>
</div>
